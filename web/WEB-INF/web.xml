<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">
    <listener>
        <listener-class>com.myFootballFacts.servlets.Config</listener-class>
    </listener>

    <servlet>
        <servlet-name>GetSquadNumbers</servlet-name>
        <servlet-class>com.myFootballFacts.servlets.GetSquadNumbers</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>


    <servlet-mapping>
        <servlet-name>GetSquadNumbers</servlet-name>
        <url-pattern>/GetSquadNumbers</url-pattern>
    </servlet-mapping>


</web-app>
